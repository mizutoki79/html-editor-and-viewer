{
    "manifest_version": 2,
    "name": "html-editor-and-viewer",
    "version": "0.0.1",
    "author": "mizutoki79",
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["background.bundle.js"],
        "persistent": false
    },
    "browser_action": {
        "name": "html-editor-and-viewer"
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}
